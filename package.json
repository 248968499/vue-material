{
  "name": "vue-material",
  "description": "Material Design for Vue.js",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "marcosmoura",
    "email": "marcosvmmoura@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuematerial/vue-material.git"
  },
  "bugs": {
    "url": "https://github.com/vuematerial/vue-material/issues"
  },
  "scripts": {
    "lerna": "lerna",
    "add-package": "scripty",
    "postinstall": "lerna bootstrap",
    "updated": "lerna updated",
    "clean": "lerna clean; rm -rf node_modules yarn.lock;",
    "dev": "cd docs; yarn dev",
    "docs-test": "cd docs; yarn test",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
		"@vue/cli-plugin-eslint": "^3.0.0-rc.3",
    "@vue/cli-service": "^3.0.0-rc.3",
		"@vue/eslint-config-standard": "^3.0.0-rc.3",
		"babel-eslint": "^8.2.3",
    "eslint-plugin-jest": "^21.15.2",
    "lerna": "3.0.0-beta.21",
    "lint-staged": "^7.1.2",
    "scripty": "^1.7.2",
    "yorkie": "^1.0.3"
  },
  "scripty": {
    "silent": true
  },
  "sideEffects": false,
  "engines": {
    "node": ">= 7.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "yarn lint",
      "git add"
    ]
  }
}
