{
  "name": "vue-material",
  "description": "Material Design for Vue.js",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "marcosmoura",
    "email": "marcosvmmoura@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuematerial/vue-material.git"
  },
  "bugs": {
    "url": "https://github.com/vuematerial/vue-material/issues"
  },
  "scripts": {
    "lerna": "lerna",
    "add-package": "scripty",
    "postinstall": "lerna bootstrap",
    "updated": "lerna updated",
    "clean": "lerna clean; rm -rf node_modules yarn.lock;",
    "dev": "cd docs; yarn dev",
    "docs-lint": "cd docs; yarn lint",
    "docs-lint-fix": "cd docs; yarn lint-fix"
  },
  "devDependencies": {
    "lerna": "3.0.0-beta.21",
    "lint-staged": "^7.1.0",
    "scripty": "^1.7.2",
    "yorkie": "^1.0.3"
  },
  "scripty": {
    "silent": true
  },
  "engines": {
    "node": ">= 7.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{ts,js,vue}": [
      "yarn docs-lint",
      "git add"
    ]
  }
}
